from random import *
cnt = 0 # number of passenger
for i in range(1, 51): # num of passenger (1~50)
    time = randrange(5, 51) # cost time (5min ~ 50min)
    if 5 <= time <= 15: # 5min ~ 15min match
        print("[0] {0}번째 손님 (소요시간 : {1}분)".format(i, time))
        cnt += 1 #count +1
    else:
        print("[ ] {0}번째 손님 (소요시간 : {1}분)".format(i, time))

print("총 탑승 승객 : {0} 분".format(cnt))
